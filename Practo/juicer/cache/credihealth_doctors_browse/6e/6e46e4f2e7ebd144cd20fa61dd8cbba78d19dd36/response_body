$(function() {
 if (false){
  $(".doc-outer").hover(function() {
  
  $('#grey-doc').attr("src", "/assets/doc-blue-50fcf52e7f16b15104657a96d3155ee2b7bc598252bc9df811850f7db8d1328f.svg");
