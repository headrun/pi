<script>
  $("#header_search_btn").click(function(e){
    e.preventDefault();
    if ($("#txtSearch").val() == ""){
      $("#txtSearch").focus();
      return
    }
    url = window.location.origin + '/doctors';
    url += "/" + city.toLowerCase();
    if (area != "")
    {
      url += "/" + area.toLowerCase();
    }
    url += "/" + $("#txtSearch").val().toLowerCase();
    url = url.replace(/ /g, "-");
    window.location.href = url
  })
  $(document).ready(function(){
    $('.top-header--left--bars').on('click', function(e){
      $('.header-leftnav').animate({"left": "0px"});
    });
    $('.header-leftnav-close').on('click', function(e){
      $('.header-leftnav').animate({"left": "-400px"});
    });
  });
</script>
