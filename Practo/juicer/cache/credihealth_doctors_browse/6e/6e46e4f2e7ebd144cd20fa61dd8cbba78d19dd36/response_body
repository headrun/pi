<script type="text/javascript">

//show more
$(document).ready(function () {
		var dynamic_name= "doctor";
	$("#btn-"+dynamic_name).click(function(){
		$('#spec-less-'+dynamic_name).hide();
		$('#view-more-'+dynamic_name).hide();
		$('#spec-more-'+dynamic_name).show();
		$('#view-less-'+dynamic_name).show();
		console.log(dynamic_name)
	});

$("#view-less-"+dynamic_name).click(function(){
	$("#spec-more-"+dynamic_name).hide();
	$("#view-less-"+dynamic_name).hide();
	$('#spec-less-'+dynamic_name).show()
	$('#view-more-'+dynamic_name).show()
	});

});


</script>


